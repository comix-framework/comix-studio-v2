    "ant-design-vue": "^3.2.0",
    "axios": "^0.27.2",
    "chart.js": "^3.7.1",
    "dayjs": "^1.11.1",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.7.0",
    "uuid": "^8.3.2",
    "vuedraggable": "^4.1.0",
    "ws": "^8.5.0"
  }
